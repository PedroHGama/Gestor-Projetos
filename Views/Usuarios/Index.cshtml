@model IEnumerable<SistemaProjetos1._3.Models.Usuario>

@{
    ViewData["Title"] = "Index";
}

<h1>Página principal - Usuários</h1>

<p>
    <a asp-action="Create">Criar novo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Nome de Usuário
                @* @Html.DisplayNameFor(model => model.NomeUsuario) *@
            </th>
            <th>
                FotoMimeType
                @* @Html.DisplayNameFor(model => model.FotoMimeType) *@
            </th>
            <th>
                Foto
                @* @Html.DisplayNameFor(model => model.Foto) *@
            </th>
            <th>
                Função
                @* @Html.DisplayNameFor(model => model.Funcao) *@
            </th>
            <th>
                Email
                @* @Html.DisplayNameFor(model => model.Email) *@
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FotoMimeType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Foto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Funcao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdUsuario">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.IdUsuario">Detalhes</a> |
                <a asp-action="Delete" asp-route-id="@item.IdUsuario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
